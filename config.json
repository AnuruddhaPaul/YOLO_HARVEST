{
  "data_path": "osu-small-animals-lila/Sorted_by_species",
  "save_dir": "./OSU_MODEL_HARVEST",
  "batch_size": 16,
  "accumulation_steps": 2,
  "num_epochs": 100,
  "image_size": 224,
  "num_workers": 0,
  "pin_memory": true,
  "base_lr": 0.0001,
  "head_lr": 0.0004,
  "weight_decay": 0.01,
  "patch_size": 16,
  "embed_dim": 192,
  "num_heads": 6,
  "dropout": 0.1,
  "stochastic_depth": 0.1,
  "save_interval": 5,
  "seed": 42,
  "label_smoothing": 0.1,
  "mixup_alpha": 0.2,
  "cutmix_alpha": 1.0,
  "amp": true,
  "warmup_epochs": 5,
  "early_stopping_patience": 15,
  "train_val_split": 0.8,
  "balance_classes": true,
  "balance_strategy": "augment_to_median",
  "use_weighted_loss": true,
  "augment_minority_classes": true,
